<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>


<BorderPane xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="it.polimi.ingsw.client.view.gui.GameEnd"
            prefHeight="400.0" prefWidth="600.0"
            stylesheets="@styles/style.css, @styles/PregameStyles.css"
            styleClass="background">

    <center>
        <VBox spacing="10" alignment="CENTER">
            <Label fx:id="message" styleClass="title" />
            <Label fx:id="leaderboard" />
            <Button text="Back to game lobby" onAction="#backToPregame" />
        </VBox>
    </center>

</BorderPane>
